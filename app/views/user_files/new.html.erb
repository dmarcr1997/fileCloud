<div class = 'container'>
    <div class='jumbotron'>
        <h1>New File</h1>
        <%= form_for @user_file, url: create_file_path(@folder),  :multipart => true do |f| %>
            <div class='form-group'>
            <%= f.file_field 'user_file', class: 'form-control'%>
            </div>
            <%= f.hidden_field :folder_id, :value => @folder.id%>
            <%= f.submit "Send File", class: "btn btn-default"%>
        <%end%>
    </div>
    <img class='img-thumbnail' src='https://images.pexels.com/photos/1370294/pexels-photo-1370294.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940' alt='filecab'>
</div>